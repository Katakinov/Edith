<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.Pane?>

<Pane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: #e1e1e1;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.edith.AppController">
    <children>
        <Pane fx:id="rootCanvas" layoutY="25.0" prefHeight="480.0" prefWidth="500.0" />
        <MenuBar>
            <menus>
                <Menu mnemonicParsing="false" text="Файл">
                    <items>
                        <MenuItem fx:id="menuItemNew" mnemonicParsing="false" onAction="#onMenuItemNew" text="Создать" />
                        <MenuItem fx:id="menuItemLoad" mnemonicParsing="false" onAction="#onMenuItemLoad" text="Загрузить сцену" />
                        <MenuItem fx:id="menuItemSave" mnemonicParsing="false" onAction="#onMenuItemSave" text="Сохранить как" />
                        <MenuItem fx:id="menuItemImport" mnemonicParsing="false" onAction="#onMenuItemImport" text="Импорт" />
                        <MenuItem fx:id="menuItemExport" mnemonicParsing="false" onAction="#onMenuItemExport" text="Экспорт" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Добавить">
                    <items>
                        <Menu fx:id="menuInputNodes" mnemonicParsing="false" text="Значения" />
                        <Menu fx:id="menuFunctionNodes" mnemonicParsing="false" text="Свойства" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>
</Pane>